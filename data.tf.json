{
  "data": {
    "oci_containerengine_cluster_option": {
      "main": { "cluster_option_id": "all" }
    },
    "oci_core_images": {
      "ubuntu": {
        "compartment_id": "${oci_identity_compartment.platform.id}",
        "filter": {
          "name": "display_name",
          "regex": true,
          "values": ["^Canonical-Ubuntu-([0-9][02468])\\.04-([0-9\\.]+-?)+$"]
        },
        "operating_system": "Canonical Ubuntu",
        "shape": "VM.Standard.E2.1.Micro",
        "sort_by": "DISPLAYNAME",
        "sort_order": "DESC"
      }
    },
    "oci_core_services": {
      "all_services": {
        "filter": {
          "name": "name",
          "regex": true,
          "values": ["All .* Services In Oracle Services Network"]
        }
      }
    },
    "oci_identity_availability_domains": {
      "main": { "compartment_id": "${var.tenancy_ocid}" }
    },
    "oci_identity_fault_domains": {
      "main": {
        "availability_domain": "${data.oci_identity_availability_domains.main.availability_domains.0.name}",
        "compartment_id": "${var.tenancy_ocid}"
      }
    }
  }
}
